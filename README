User.create(:login => 'lopo', :name => 'lopo', :email => 'lopo@dream2.com', :password => 'xxx', :password_confirmation => 'xxx')
User.create(:login => 'logo', :name => 'logo', :email => 'logo@dream2.com', :password => 'xxx', :password_confirmation => 'xxx')

